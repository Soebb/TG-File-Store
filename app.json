{
  "name": "File Store",
  "description": "Telegram bot to store your telegram files",
  "keywords": [
    "telegram",
    "best",
    "Store",
    "Files"
  ],
  "logo": "https://telegra.ph/file/bdd3352951be090a56590.jpg",
  "success_url": "https://telegram.dog/filestore_Nsbot",
  "website": "https://github.com/Ns-Bots/TG-File-Store",
  "repository": "https://github.com/Ns-Bots/TG-File-Store",
  "env": {
    "WEBHOOK": {
      "description": "Setting this to ANYTHING will enable webhooks when in env mode",
      "value": "ANYTHING"
    },
    "BOT_TOKEN": {
      "description": "Your bot token, as a string.",
      "value": ""
    },
    "API_ID": {
      "description": "Get this value from https://my.telegram.org",
      "value": ""
    },
    "DB_CHANNEL_ID": {
      "description": "add a telegram channel ID as the database",
      "value": ""
    },
    "LOG_CHANNEL": {
      "description": "another channel ID",
      "value": ""
    },
    "API_HASH": {
      "description": "Get this value from https://my.telegram.org",
      "value": ""
    }
  },
  "addons": [
  {
    "plan": "heroku-postgresql",
    "options": {"version": "12"}
  }
  ],
  "buildpacks": [{
    "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
  }, {
    "url": "heroku/python"
  }],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  }
}
